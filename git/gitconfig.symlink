[init]
  templatedir = ~/.git_templates
[alias]
  aa = add --all
  ci = commit -v
  cl = clone
  co = checkout
  st = status
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore_global
  autocrlf = input
[fetch]
  prune = true
[include]
  path = ~/.gitconfig.local
[merge]
  ff = only
[pull]
  default = current
[push]
  default = current
[credential]
	helper = osxkeychain
